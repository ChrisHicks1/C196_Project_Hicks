<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="android.christopherapp.c196projecthicks">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_baseline_library_books_24"
        android:supportsRtl="true"
        android:theme="@style/Theme.C196ProjectHicks"
        tools:targetApi="31">
        <activity
            android:name=".UI.CourseInfo"
            android:parentActivityName=".UI.Home"
            android:label="Course Information"
            android:exported="false" />
        <activity
            android:name=".UI.CourseList"
            android:parentActivityName=".UI.Home"
            android:label="Course List"
            android:exported="false" />
        <activity
            android:name=".UI.TermList"
            android:parentActivityName=".UI.Home"
            android:label="Term List"
            android:exported="false" />
        <activity
            android:name=".UI.TermInfo"
            android:parentActivityName=".UI.Home"
            android:label="Term Information"
            android:exported="false" />
        <activity
            android:name=".UI.Home"
            android:parentActivityName=".UI.MainActivity"
            android:exported="false" />
        <activity
            android:name=".UI.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>